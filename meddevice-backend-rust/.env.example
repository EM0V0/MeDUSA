# Environment Configuration Template for MeDUSA Backend
# Copy this file to .env and fill in your specific values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Application Environment
ENVIRONMENT=development

# JWT Configuration (will be auto-generated if not provided)
# Generate a secure secret: openssl rand -base64 64
# JWT_SECRET=your-secure-jwt-secret-here

# Database Configuration (auto-managed by SAM)
# USERS_TABLE=development-meddevice-users
# PATIENTS_TABLE=development-meddevice-patients
# DEVICES_TABLE=development-meddevice-devices
# REPORTS_TABLE=development-meddevice-reports
# AUDIT_LOGS_TABLE=development-meddevice-audit-logs

# S3 Bucket Configuration (auto-managed by SAM)
# REPORTS_BUCKET=development-meddevice-reports-{account-id}
# DEVICE_DATA_BUCKET=development-meddevice-device-data-{account-id}
# BACKUP_BUCKET=development-meddevice-backups-{account-id}

# Logging
RUST_LOG=info

# Security Settings
ENABLE_WAF=false
ENABLE_VPC=false
ALLOWED_ORIGINS=*